# Dart / Flutter
.dart_tool/
.packages
build/
pubspec.lock

# Docs
doc/api/

# dotenv environment variables file
.env*

# Avoid committing generated Javascript files
*.dart.js
*.info.json
*.js
*.js_
*.js.deps
*.js.map

# Flutter generated plugin files
.flutter-plugins
.flutter-plugins-dependencies

# Emscripten / third_party build caches
emsdk/
third_party/*/build*/
third_party/*/out*/

# Ignore generated object files, archives, and temp build files
*.o
*.obj
*.lo
*.la
*.al
*.libs
*.d
*.s
*.tmp

# macOS temp files
.DS_Store

# Android NDK cache (do not commit)
.ndk/
.ndk/**

# ---------- Keep required runtime binaries (un-ignore) ----------
# Mobile: ship prebuilt .so
!android/src/main/jniLibs/**/*.so

# iOS: vendored static lib
!ios/**/libx448dart.a

# Web: WASM + loader JS must be in the package
!web/x448_wasm_bg.wasm
!web/x448_wasm.js
